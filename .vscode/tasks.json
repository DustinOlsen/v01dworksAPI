{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run FastAPI",
			"type": "shell",
			"command": "${workspaceFolder}/.venv/bin/python",
			"args": [
				"-m",
				"uvicorn",
				"main:app",
				"--reload",
				"--port",
				"8011"
			],
			"isBackground": true,
			"problemMatcher": [
				"$python"
			],
			"group": "none"
		}
	]
}